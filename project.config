[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group OP 3 Maintainers
	owner = group user/Tab Fitts (tabp0le)
[access "refs/for/refs/*"]
	read = group OP 3 Maintainers
	read = group user/Tab Fitts (tabp0le)
	push = group OP 3 Maintainers
	push = group user/Tab Fitts (tabp0le)
	label-Code-Review = -2..+2 group OP 3 Maintainers
	label-Code-Review = -2..+2 group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group OP 3 Maintainers
	label-Verified = -1..+1 group user/Tab Fitts (tabp0le)
	rebase = group OP 3 Maintainers
	rebase = group user/Tab Fitts (tabp0le)
	removeReviewer = group OP 3 Maintainers
	removeReviewer = group user/Tab Fitts (tabp0le)
	submit = group OP 3 Maintainers
	submit = group user/Tab Fitts (tabp0le)
[access "refs/heads/*"]
	read = group OP 3 Maintainers
	read = group user/Tab Fitts (tabp0le)
	create = group OP 3 Maintainers
	create = group user/Tab Fitts (tabp0le)
	push = +force group OP 3 Maintainers
	push = +force group user/Tab Fitts (tabp0le)
	pushMerge = group OP 3 Maintainers
	pushMerge = group user/Tab Fitts (tabp0le)
	label-Verified = -1..+1 group OP 3 Maintainers
[access "refs/tags/*"]
	push = +force group OP 3 Maintainers
	push = group user/Tab Fitts (tabp0le)
	push = +force group user/Tab Fitts (tabp0le)
[submit]
	action = rebase if necessary
[receive]
	requireChangeId = false
